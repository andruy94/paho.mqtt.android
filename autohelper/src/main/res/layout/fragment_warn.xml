<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    tools:context="com.a1101studio.autohelper.WarnFragment">




    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:layout_width="384dp"
        android:layout_height="21dp"
        android:text="Введите номер автомибиля"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/textView" />

    <EditText
        android:lines="1"
        android:layout_width="384dp"
        android:layout_height="54dp"
        android:inputType="textPersonName"
        android:hint="EX101S777"
        android:ems="10"
        android:id="@+id/editText"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintLeft_toLeftOf="@+id/textView" />


    <Button
        android:text="Я вас запер"
        android:id="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/multiAutoCompleteTextView"
        app:layout_constraintRight_toRightOf="@+id/multiAutoCompleteTextView"
        style="@style/button"
        android:layout_height="100dp"
        android:layout_width="100dp"
        app:layout_constraintLeft_toLeftOf="@+id/multiAutoCompleteTextView" />

    <MultiAutoCompleteTextView
        android:minLines="5"
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:id="@+id/multiAutoCompleteTextView"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        app:layout_constraintRight_toRightOf="@+id/textView2" />

    <TextView
        android:text="Введите сообщение"
        android:layout_width="384dp"
        android:layout_height="25dp"
        android:id="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        app:layout_constraintLeft_toLeftOf="@+id/editText" />


</android.support.constraint.ConstraintLayout>
